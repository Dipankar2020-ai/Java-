import java.lang.*;
import java.util.*;
class Binarytodecimal
{
   public static void main(String args[])
    {
       Scanner s=new Scanner(System.in);
       s.useRadix(2);
       System.out.println("Enter the number in binary format");
       int x=s.nextInt();
       System.out.println("Decimal value is :"+x);
    }
}
